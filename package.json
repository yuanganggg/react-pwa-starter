{
  "name": "react-starter",
  "version": "0.1.0",
  "description": "react-starter",
  "repository": "https://github.com/bornkiller/preact-seed.git",
  "author": "bornkiller <hjj491229492@hotmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "clean": "rm -rf bundle",
    "dev": "npm-run-all -p dev:*",
    "dev:client": "coco server --react --bootstrap ./src/main.jsx",
    "dev:ssr": "coco ssr --watch --server --bootstrap ./server/main.jsx",
    "build": "npm-run-all -p build:*",
    "build:client": "coco build --bootstrap ./src/main.jsx",
    "build:ssr": "coco ssr --bootstrap ./server/main.jsx",
    "test": "jest -c jest.config.js --coverage",
    "test:jest": "jest -c jest.config.js --watch",
    "lint": "npm-run-all -p lint:*",
    "lint:source": "eslint --fix src/",
    "lint:test": "eslint --fix __tests__/"
  },
  "devDependencies": {
    "@coco/cli": "^0.3.16",
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-inline-environment-variables": "^0.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0 || ^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.1",
    "npm-run-all": "^4.0.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "webpack-plugin-inject-service-worker": "^0.2.3"
  },
  "browserslist": [
    "last 2 versions",
    "iOS 10"
  ],
  "dependencies": {
    "antd": "^2.12.6",
    "babel-runtime": "^6.23.0",
    "connect-history-api-fallback": "^1.3.0",
    "express": "^4.15.4",
    "history": "^4.6.3",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-router-dom": "^4.1.2",
    "rxjs": "^5.3.0"
  },
  "peerDependencies": {}
}
